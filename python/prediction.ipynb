   "source": [
    "import os\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import pickle\n",
    "from flask import Flask, request, jsonify\n",
    "from flask_cors import CORS\n",
    "from keras.models import load_model\n",
    "\n",
    "app=Flask(__name__)\n",
    "CORS(app)\n",
    "\n",
    "# Path where user data will be stored\n",
    "#user_data_path = path where you want to save the user data,
    "os.chdir(user_data_path)\n",
    "model=load_model(r'final_adverse_reaction_prediction_model.keras')\n",
    "\n",
    "demoinputs,indidruginputs=None,None\n",
    "# Load the drug and indication token maps\n",
    "with open('drugmap.pkl', 'rb') as f:\n",
    "    drug_map = pickle.load(f)\n",
    "\n",
    "with open('indimap.pkl', 'rb') as f:\n",
    "    indi_map = pickle.load(f)\n",
    "with open('reacptmap.pkl','rb') as f:\n",
    "    reacpt_map=pickle.load(f)\n",
    "# Define the columns for the \"indications_and_drugs\" DataFrame\n",
    "indi_drug_columns = [\n",
    "    'caseid', 'drug_seq', 'dechal_D', 'dechal_N', 'dechal_U', 'dechal_Y',\n",
    "    'rechal_D', 'rechal_N', 'rechal_U', 'rechal_Y', 'indi_pt_id', 'drugname_id'\n",
    "]\n",
    "\n",
    "# Define the columns for the \"demographics\" DataFrame\n",
    "demo_columns = [\n",
    "    'caseid', 'wt', 'sex_F', 'sex_M', 'sex_UNK', 'sex_sexna', \n",
    "    'age_grp_A', 'age_grp_C', 'age_grp_E', 'age_grp_I', \n",
    "    'age_grp_N', 'age_grp_T', 'age_grp_age_grpna'\n",
    "]\n",
    "\n",
    "# Flask application setup\n",
    "# app = Flask(_name_)\n",
    "\n",
    "# Class to create directories and files for storing patient data\n",
    "class Patient:\n",
    "    def __init__(self, caseid, demo=None, indications_and_drugs=None):\n",
    "        self.caseid = caseid\n",
    "        self.demo = demo  # Expecting demo to be a dictionary with demographic data\n",
    "        self.indications_and_drugs = indications_and_drugs\n",
    "    def calculate_age_grp(self):\n",
    "        \"\"\"\n",
    "        Calculate age group based on the provided age.\n",
    "        Returns:\n",
    "            str: Age group code ('A', 'C', 'E', 'I', 'N', 'T', 'age_grpna')\n",
    "        \"\"\"\n",
    "        if self.demo.get('age') is None or pd.isna(self.demo.get('age')):\n",
    "            return 'age_grpna'\n",
    "        elif self.demo.get('age') <= 0.077:  # Approx 27 days (Neonate)\n",
    "            return 'N'\n",
    "        elif 0.077 < self.demo.get('age') <= 2:  # Approx 23 months (Infant)\n",
    "            return 'I'\n",
    "        elif 2 < self.demo.get('age') <= 11:  # Child\n",
    "            return 'C'\n",
    "        elif 12 <= self.demo.get('age') <= 17:  # Teenager\n",
    "            return 'T'\n",
    "        elif 18 <= self.demo.get('age') <= 64:  # Adult\n",
    "            return 'A'\n",
    "        elif self.demo.get('age') >= 65:  # Elderly\n",
    "            return 'E'\n",
    "        else:\n",
    "            return 'age_grpna'\n",
    "        \n",
    "    def create_patient_directory(self):\n",
    "        global demoinputs\n",
    "        user_dir = os.path.join(user_data_path, str(self.caseid))\n",
    "        if not os.path.exists(user_dir):\n",
    "            os.mkdir(user_dir)\n",
    "        \n",
    "        # Initialize empty DataFrames for demographics and indications/drugs\n",
    "        demo_df = pd.DataFrame(columns=demo_columns)\n",
    "        # indi_drug_df = pd.DataFrame(columns=indi_drug_columns)\n",
    "\n",
    "        # If demo data is provided, fill in the demographics DataFrame\n",
    "        self.demo['age_grp']=self.calculate_age_grp()\n",
    "        if self.demo:\n",
    "            demo_data = {\n",
    "                'caseid': self.caseid,\n",
    "                'wt': self.demo.get('wt', None),\n",
    "                'sex_F': 1 if self.demo.get('sex') == 'female' else 0,\n",
    "                'sex_M': 1 if self.demo.get('sex') == 'male' else 0,\n",
    "                'sex_UNK': 1 if self.demo.get('sex') == 'UNK' else 0,\n",
    "                'sex_sexna': 1 if self.demo.get('sex') == 'sexna' else 0,\n",
    "                'age_grp_A': 1 if self.demo.get('age_grp') == 'A' else 0,\n",
    "                'age_grp_C': 1 if self.demo.get('age_grp') == 'C' else 0,\n",
    "                'age_grp_E': 1 if self.demo.get('age_grp') == 'E' else 0,\n",
    "                'age_grp_I': 1 if self.demo.get('age_grp') == 'I' else 0,\n",
    "                'age_grp_N': 1 if self.demo.get('age_grp') == 'N' else 0,\n",
    "                'age_grp_T': 1 if self.demo.get('age_grp') == 'T' else 0,\n",
    "                'age_grp_age_grpna': 1 if self.demo.get('age_grp') == 'age_grpna' else 0\n",
    "            }\n",
    "\n",
    "            demo_df = pd.DataFrame([demo_data])\n",
    "            demoinputs=np.array(demo_df.drop(columns=['caseid']))\n",
    "        # Save empty or filled DataFrames to parquet files\n",
    "        demo_df.to_parquet(os.path.join(user_dir, 'demographics.parquet'))\n",
    "        \n",
    "        # indi_drug_df.to_parquet(os.path.join(user_dir, 'indications_and_drugs.parquet'))\n",
    "\n",
    "# Class to add new drug information\n",
    "class IndicationsAndDrugs(Patient):\n",
    "    def __init__(self, caseid, drugname, indication, dechal, rechal):\n",
    "        super().__init__(caseid)\n",
    "        self.drugname = drugname.lower().strip()\n",
    "        self.indication = indication.lower().strip()\n",
    "        self.dechal = dechal\n",
    "        self.rechal = rechal\n",
    "        self.store_indications_and_drugs()\n",
    "\n",
    "    def store_indications_and_drugs(self):\n",
    "        global indidruginputs\n",
    "        user_dir = os.path.join(user_data_path, str(self.caseid))\n",
    "        indi_drug_path = os.path.join(user_dir, 'indications_and_drugs.parquet')\n",
    "        \n",
    "        # Load existing DataFrame if it exists; otherwise, create a DataFrame with four rows of zeros\n",
    "        if os.path.exists(indi_drug_path):\n",
    "            indi_drug_df = pd.read_parquet(indi_drug_path)\n",
    "        else:\n",
    "            # Create a DataFrame with four rows of zeros for padding\n",
    "            indi_drug_df = pd.DataFrame(0, index=range(4), columns=indi_drug_columns)\n",
    "            # Set the caseid column to the current case ID\n",
    "            indi_drug_df['caseid'] = self.caseid\n",
    "\n",
    "        # Define the new data row with tokens\n",
    "        drug_token = drug_map.get(self.drugname, 0)\n",
    "        indi_token = indi_map.get(self.indication, 0)\n",
    "\n",
    "        new_row = {\n",
    "            'caseid': self.caseid,\n",
    "            'drug_seq': len(indi_drug_df) + 1,  # Incremental drug sequence\n",
    "            'dechal_D': 1 if self.dechal == 'D' else 0,\n",
    "            'dechal_N': 1 if self.dechal == 'N' else 0,\n",
    "            'dechal_U': 1 if self.dechal == 'U' else 0,\n",
    "            'dechal_Y': 1 if self.dechal == 'Y' else 0,\n",
    "            'rechal_D': 1 if self.rechal == 'D' else 0,\n",
    "            'rechal_N': 1 if self.rechal == 'N' else 0,\n",
    "            'rechal_U': 1 if self.rechal == 'U' else 0,\n",
    "            'rechal_Y': 1 if self.rechal == 'Y' else 0,\n",
    "            'drugname_id': drug_token,\n",
    "            'indi_pt_id': indi_token\n",
    "        }\n",
    "\n",
    "        # Append new row to DataFrame\n",
    "        indi_drug_df = pd.concat([indi_drug_df, pd.DataFrame([new_row])], ignore_index=True)\n",
    "        indi_drug_df = indi_drug_df[indi_drug_columns]\n",
    "        indidruginputs=np.array(indi_drug_df.drop(columns=['caseid']))\n",
    "        # Save the updated DataFrame to parquet file\n",
    "        indi_drug_df.to_parquet(indi_drug_path)\n",
    "\n",
    "@app.route('/predict', methods=['POST'])\n",
    "def predict():\n",
    "    data = request.get_json()\n",
    "\n",
    "    # Extract user data from the request\n",
    "    caseid = data.get('username')\n",
    "    age = data.get('age')\n",
    "    sex = data.get('gender')\n",
    "    weight = data.get('weight')\n",
    "    drugname = data.get('drugName')\n",
    "    indication = data.get('diseaseName')\n",
    "    dechal = data.get('dechallanged')\n",
    "    rechal = data.get('rechallenge')\n",
    "\n",
    "    # Create patient directory and save demo information\n",
    "    demo_data = {\n",
    "        'wt': weight,\n",
    "        'sex': sex,\n",
    "        'age': age\n",
    "    }\n",
    "    patient = Patient(caseid=caseid, demo=demo_data)\n",
    "    patient.create_patient_directory()\n",
    "\n",
    "    # Add drug and indication information\n",
    "    indications_and_drugs = IndicationsAndDrugs(\n",
    "        caseid=caseid, \n",
    "        drugname=drugname, \n",
    "        indication=indication, \n",
    "        dechal=dechal, \n",
    "        rechal=rechal\n",
    "    )\n",
    "    reshaped_indidruginputs = np.expand_dims(indidruginputs[-5:], axis=0)  # Shape: (1, 5, 11)\n",
    "    prediction = model.predict([demoinputs, reshaped_indidruginputs])\n",
    "    indices=np.where(prediction[0]>0.01)\n",
    "    possiblereactions=[]\n",
    "    for token in indices[0]:\n",
    "        for reaction in reacpt_map:\n",
    "            if reacpt_map[reaction] ==token:\n",
    "                # message = f'{reaction}{\" \"*(40-len(reaction))}Probability:{min(99, round(prediction[0][token] * 400, 2))}%'\n",
    "                message = f'{reaction}{\" \"*(80-len(reaction))}Probability:{min(99, prediction[0][token] * 400):.2f}%'\n",
    "\n",
    "                possiblereactions.append(message + \"\\n\")\n",
    "\n",
    "                break\n",
    "    if possiblereactions==[]:\n",
    "        return jsonify({\"prediction\":[\"No risk identified\"]})\n",
    "    return jsonify({\"prediction\": possiblereactions})\n",
    "\n",
    "# #Run the Flask app\n",
    "if __name__ == '__main__':\n",
    "    app.run(host='0.0.0.0', port=3000)\n"
   ]
  },
